@using System.Web.Optimization
@{
   // ViewBag.Title = "InternalServerErrorDisable";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-CompatiblSalve" content="IE=edge">
    <title></title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <script src="https://kit.fontawesome.com/93c5f0292a.js" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <link href="~/Content/css/account/login.css" rel="stylesheet" />
    <link rel="icon" href="@Url.Content("~/Img/ut-logo.ico")" />
    @Styles.Render("~/Bundles/css")
</head>
<body>
    <noscript>
        <meta http-equiv="refresh" content="0;URL=@Url.Action("NoScript", "Error")" />
    </noscript>

    <div class="error-page">
        <h2 class="headline text-red">500</h2>

        <div class="error-content">
            <h3><i class="fa fa-warning text-red"></i> Oops! Algo salio mal al procesar tu solicitud.</h3>
            <p>
                Se encontro un error al procesar la información que enviaste, por favor intenta de nuevo.<br />
                Si el problema persiste, contacta al administrador del sitio.
            </p>


            <a href="@Url.Action("Login", "Account")" class="btn btn-default" role="button">Regresar</a>
        </div>
    </div>

    @Scripts.Render("~/Bundles/js")
</body>
</html>

